<%- include('partials/header'); -%>

    <h1>Home</h1>
    <p>
        <%= StartingContent %>
    </p>
    <p>
        <% posts.forEach(function (post) {%>
            <h1>
                <%= post.title %>
                    <!-- post.title -->
            </h1>
            <p>
                <%= post.content.substring(0, 100) + "..." %>
                    <!-- post.content -->
                    <a href="/posts/<%= post.title %>">Read More</a>
            </p>
            <% });%>
    </p>

    <%- include('partials/footer'); -%>